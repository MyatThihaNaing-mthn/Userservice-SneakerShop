{
  "spring": {
    "application": {
      "name": "UserService"
    }
  },
  "UserDatabaseSetting": {
    "ConnectionName": "mongodb://localhost:27017",
    "DatabaseName": "UserService",
    "CollectionName": "Users"
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://localhost:8010/eureka",
      "ValidateCertificates":  false
    },
    "instance" : {
      "appName" : "UserService",
      "hostName" : "localhost"
    } 
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
